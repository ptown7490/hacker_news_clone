<%= render('layouts/errors', object: @link) %>

<%= form_for(@link) do |f| %>
  <div class='form-group'>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div class='form-group'>
    <%= f.label :content_url %>
    <%= f.text_field :content_url %>
  </div>
  <%= f.submit(:class => 'btn btn-primary') %>
<% end %>
